<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
	<hibernate-mapping>
		<class name="com.dxr.system.entity.Iconlibrary" table="SYS_ICON_LIBRARY">
			<id name="id" type="java.lang.String">
				<column name="ID" length="32" />
				<generator class="uuid" />
			</id>
			
			<property name="name" type="java.lang.String">
				<column name="NAME" length="100" />
			</property>
			
			<property name="iconCls" type="java.lang.String">
				<column name="ICON_CLS" length="100" />
			</property>
			
			<property name="from" type="java.lang.String">
				<column name="ICON_FROM" length="20" />
			</property>
			
			<property name="createDate" type="java.util.Date">
				<column name="CREATE_DATE" />
			</property>
			
			<property name="updateDate" type="java.util.Date">
				<column name="UPDATE_DATE" />
			</property>
			
			<property name="sort" type="java.lang.Integer">
				<column name="SORT" />
			</property>
		</class>
	</hibernate-mapping>