			<property name="ext02" type="java.lang.String">
				<column name="EXT_02" length="1024" />
			</property>
			<property name="ext03" type="java.lang.String">
				<column name="EXT_03" length="1024" />
			</property>
			<property name="ext04" type="java.lang.String">
